    "crypto-js": "^4.1.1",
    "dompurify": "^2.3.6",
    "ethers": "^5.5.4",
    "graphql": "^16.5.0",
    "graphql-request": "^4.2.0",
    "html-react-parser": "^1.4.8",
    "moment": "^2.29.1",
    "react": "17.x",
